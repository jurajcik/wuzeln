          "rawSql": "WITH winner_count as (\r\nSELECT\r\n count(m.id) AS \"value\",\r\n  pe.nickname AS nickname\r\nFROM wuzeln.person pe \r\n  JOIN wuzeln.player p ON p.person_id = pe.id\r\n  JOIN wuzeln.team t on t.id = p.team_id\r\n  JOIN wuzeln.match m on m.id = t.match_id\r\nWHERE\r\n  $__timeFilter(m.createddate) AND\r\n   m.enddate is not null AND\r\n  t.winner = TRUE\r\nGROUP BY pe.nickname\r\n  ), \r\nall_count as (\r\nSELECT\r\n count(m.id) AS \"value\",\r\n  pe.nickname AS nickname\r\nFROM wuzeln.person pe \r\n  JOIN wuzeln.player p ON p.person_id = pe.id\r\n  JOIN wuzeln.team t on t.id = p.team_id\r\n  JOIN wuzeln.match m on m.id = t.match_id\r\nWHERE\r\n  $__timeFilter(m.createddate) AND\r\n   m.enddate is not null\r\nGROUP BY pe.nickname\r\n  )\r\n  \r\nSELECT wc.value / cast(ac.value as float) as value, \r\nac.nickname as metric,\r\nnow() as time\r\nFROM winner_count wc \r\nJOIN all_count ac ON wc.nickname = ac.nickname\r\nORDER BY value DESC\r\n",
